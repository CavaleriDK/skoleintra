{
  "name": "skoleintra",
  "version": "1.1.1",
  "description": "Node module to fetch data from ItsLearnings skoleintra platform",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc && cp -r src/ dist/src && rm -rf dist/src/__tests__",
    "format": "prettier --write \"src/**/*.(js|ts)\"",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --fix --ext .js,.ts",
    "test": "jest --detectOpenHandles --forceExit --config jest.config.js",
    "test:debug": "SKOLEINTRA_TEST_WITH_DEBUG=true jest --detectOpenHandles --forceExit --config jest.config.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CavaleriDK/skoleintra.git"
  },
  "keywords": [
    "skoleintra",
    "for√¶ldreintra",
    "itslearning"
  ],
  "author": "Michael Cavaleri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CavaleriDK/skoleintra/issues"
  },
  "homepage": "https://github.com/CavaleriDK/skoleintra",
  "devDependencies": {
    "@types/jest": "29.5.12",
    "@types/puppeteer": "7.0.4",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "dotenv": "16.4.5",
    "eslint": "8.56.0",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "prettier": "3.2.5",
    "ts-jest": "29.1.2",
    "typescript": "5.3.3"
  },
  "files": [
    "dist/**/*",
    "src/index",
    "src/helpers/**/*"
  ],
  "dependencies": {
    "puppeteer-core": "22.2.0",
    "puppeteer-extra": "3.3.6",
    "puppeteer-extra-plugin-stealth": "2.11.2"
  }
}
